<td class="text-end">
	<div class="btn-group" role="group">
		<a type="button" class="btn btn-primary" href="@Url.Action("Edit", new { id = Model })"><i class="fa-duotone fa-regular fa-pen-to-square"></i></a>
		<form method="post" actoin="Delete">
			<button onclick="if(!confirm('Are you sure you want to delete this Item')){return false}" type="submit" class="btn btn-danger rounded-0" asp-action="Delete" asp-route-id="@Model"><i class="fa-solid fa-regular fa-trash"></i></button>
		</form>
	</div>
</td>
